{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useState } from 'react';\nimport Search from '../../search/Search';\nimport axios from 'axios';\nimport SERVER from '../../../client/utils/url';\nimport HostList from './HostList';\nexport default function HostPage(props) {\n  const {\n    items\n  } = props;\n  const {\n    0: foundItems,\n    1: setFoundItems\n  } = useState(items);\n\n  const onSubmit = async (event, type, item) => {\n    event.preventDefault();\n    const res = await axios.post(`${SERVER}/api/host/search`, {\n      type: type,\n      item: item\n    });\n    setFoundItems(res.data.users);\n  };\n\n  return /*#__PURE__*/_jsxs(\"div\", {\n    children: [/*#__PURE__*/_jsx(Search, {\n      options: ['email', 'nickname', 'name'],\n      label: ['이메일', '닉네임', '이름'],\n      onSubmit: onSubmit,\n      selectLabel: \"\\uAC80\\uC0C9\\uD560 \\uAC12\",\n      marginBottom: \"2em\"\n    }), /*#__PURE__*/_jsx(HostList, {\n      items: foundItems.length !== 0 ? foundItems : []\n    })]\n  });\n}","map":null,"metadata":{},"sourceType":"module"}