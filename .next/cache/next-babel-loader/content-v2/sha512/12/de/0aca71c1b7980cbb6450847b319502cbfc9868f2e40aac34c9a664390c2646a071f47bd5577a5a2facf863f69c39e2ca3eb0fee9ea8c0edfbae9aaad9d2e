{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport Button from '@material-ui/core/Button';\nimport axios from 'axios';\nimport Router from 'next/router';\nimport React, { useState } from 'react';\nimport styled from 'styled-components';\nimport SERVER from '../../../client/utils/url';\nconst MainTitle = styled.h2.withConfig({\n  displayName: \"NoticeUpdate__MainTitle\",\n  componentId: \"sc-11cqhy5-0\"\n})([\"color:#5197d5;text-align:center;margin-top:0;margin-bottom:3em;padding-top:2em;border-top:3px solid #5197d5;\"]);\nconst SubTitle = styled.h6.withConfig({\n  displayName: \"NoticeUpdate__SubTitle\",\n  componentId: \"sc-11cqhy5-1\"\n})([\"color:#5197d5;text-indent:0.1rem;text-align:left;margin-top:2rem;margin-bottom:0.75rem;display:block;\"]);\nconst Title = styled.textarea.withConfig({\n  displayName: \"NoticeUpdate__Title\",\n  componentId: \"sc-11cqhy5-2\"\n})([\"font-family:auto;width:98%;height:1.2rem;padding:0.25rem;align-items:left;resize:none;overflow:hidden;&:focus{outline-color:#5197d5;}\"]);\nconst Description = styled.textarea.withConfig({\n  displayName: \"NoticeUpdate__Description\",\n  componentId: \"sc-11cqhy5-3\"\n})([\"font-family:auto;width:98%;height:15rem;text-align:left;margin-bottom:1em;margin-left:0.05em;padding:0.25rem;font-size:0.9em;resize:none;overflow:hidden;&:focus{outline-color:#5197d5;}\"]);\nconst ModifyButton = styled(Button).withConfig({\n  displayName: \"NoticeUpdate__ModifyButton\",\n  componentId: \"sc-11cqhy5-4\"\n})([\"&.MuiButton-root{display:block;margin-right:0.5rem;margin-left:auto;margin-bottom:2rem;width:4rem;}&.MuiButton-containedPrimary{background-color:#5197d5;}\"]);\nconst DetailsDiv = styled.div.withConfig({\n  displayName: \"NoticeUpdate__DetailsDiv\",\n  componentId: \"sc-11cqhy5-5\"\n})([\"border-bottom:3px solid #5197d5;\"]);\n\nconst modifyNotice = (id, title, description) => {\n  axios.post(`${SERVER}/api/notice/update`, {\n    id: id,\n    title: title,\n    description: description\n  }).then(res => {\n    if (res.data.success) {\n      Router.push('/admin/notice/list');\n    }\n\n    alert(res.data.message);\n  });\n};\n\nexport default function NoticeUpdate(props) {\n  const {\n    item: notice,\n    visibility\n  } = props;\n  const {\n    0: titleState,\n    1: setTitleState\n  } = useState(notice.title);\n  const {\n    0: descState,\n    1: setDescState\n  } = useState(notice.description);\n\n  const modifyButtonHandler = e => {\n    e.preventDefault();\n    modifyNotice(notice.id, titleState, descState);\n  };\n\n  if (visibility === true) {\n    return /*#__PURE__*/_jsxs(DetailsDiv, {\n      children: [/*#__PURE__*/_jsx(MainTitle, {\n        children: \"\\uACF5\\uC9C0 \\uC218\\uC815\"\n      }), /*#__PURE__*/_jsx(SubTitle, {\n        children: \"\\uC81C\\uBAA9\"\n      }), /*#__PURE__*/_jsx(Title, {\n        autoFocus: true,\n        value: titleState,\n        onChange: e => setTitleState(e.target.value)\n      }), /*#__PURE__*/_jsx(SubTitle, {\n        children: \"\\uB0B4\\uC6A9\"\n      }), /*#__PURE__*/_jsx(Description // placeholder={item.description}\n      , {\n        value: descState,\n        onChange: e => setDescState(e.target.value)\n      }), /*#__PURE__*/_jsx(ModifyButton, {\n        type: \"submit\",\n        onClick: modifyButtonHandler,\n        variant: \"contained\",\n        color: \"primary\",\n        children: \"\\uC218\\uC815\"\n      })]\n    });\n  } else {\n    return /*#__PURE__*/_jsx(\"div\", {});\n  }\n}","map":null,"metadata":{},"sourceType":"module"}