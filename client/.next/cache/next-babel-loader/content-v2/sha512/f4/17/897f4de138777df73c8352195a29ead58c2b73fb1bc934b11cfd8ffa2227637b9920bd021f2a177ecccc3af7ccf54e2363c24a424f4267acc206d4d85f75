{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport Button from '@material-ui/core/Button';\nimport axios from 'axios';\nimport Router from 'next/router';\nimport React, { useState } from 'react';\nimport styled from 'styled-components';\nimport SERVER from '../../../utils/url';\nconst SubTitle = styled.h6.withConfig({\n  displayName: \"NoticeWrite__SubTitle\",\n  componentId: \"avaoos-0\"\n})([\"color:#5197d5;text-indent:0.1em;text-align:left;margin:0 0 0.5em 0.25em;display:block;\"]);\nconst Title = styled.textarea.withConfig({\n  displayName: \"NoticeWrite__Title\",\n  componentId: \"avaoos-1\"\n})([\"font-family:auto;width:98%;height:1.2em;padding:0.5em;align-items:left;margin-bottom:2em;resize:none;overflow:hidden;&:focus{outline-color:#5197d5;}\"]);\nconst Description = styled.textarea.withConfig({\n  displayName: \"NoticeWrite__Description\",\n  componentId: \"avaoos-2\"\n})([\"font-family:auto;width:98%;height:15em;text-align:left;margin-left:0.05em;padding:0.5em;font-size:0.9em;resize:none;overflow:hidden;&:focus{outline-color:#5197d5;}\"]);\nconst WriteButton = styled(Button).withConfig({\n  displayName: \"NoticeWrite__WriteButton\",\n  componentId: \"avaoos-3\"\n})([\"&.MuiButton-root{float:right;display:block;margin:4em 0 2em 0;width:5em;}&.MuiButton-containedPrimary{background-color:#5197d5;}\"]);\nconst DetailsDiv = styled.div.withConfig({\n  displayName: \"NoticeWrite__DetailsDiv\",\n  componentId: \"avaoos-4\"\n})([\"\"]);\n\nconst writeNotice = (title, description) => {\n  axios.post(`${SERVER}/api/notice/write`, {\n    title: title,\n    description: description\n  }).then(res => {\n    if (res.data.success) {\n      alert('공지 작성 완료');\n      Router.push('/admin/notice/list');\n    }\n  });\n};\n\nexport default function NoticeWrite() {\n  const {\n    0: titleState,\n    1: setTitleState\n  } = useState('');\n  const {\n    0: descState,\n    1: setDescState\n  } = useState('');\n\n  const writeButtonHandler = e => {\n    e.preventDefault();\n    if (titleState === '') alert('제목이 비어있습니다.');else if (descState === '') alert('내용이 비어있습니다.');else writeNotice(titleState, descState);\n  };\n\n  return /*#__PURE__*/_jsxs(DetailsDiv, {\n    children: [/*#__PURE__*/_jsx(SubTitle, {\n      children: \"\\uC81C\\uBAA9\"\n    }), /*#__PURE__*/_jsx(Title, {\n      autoFocus: true,\n      placeholder: \"\\uC81C\\uBAA9\\uC744 \\uC785\\uB825\\uD574\\uC8FC\\uC138\\uC694.\",\n      onChange: e => setTitleState(e.target.value)\n    }), /*#__PURE__*/_jsx(SubTitle, {\n      children: \"\\uB0B4\\uC6A9\"\n    }), /*#__PURE__*/_jsx(Description, {\n      placeholder: \"\\uB0B4\\uC6A9\\uC744 \\uC785\\uB825\\uD574\\uC8FC\\uC138\\uC694.\",\n      onChange: e => setDescState(e.target.value)\n    }), /*#__PURE__*/_jsx(WriteButton, {\n      type: \"submit\",\n      onClick: writeButtonHandler,\n      variant: \"contained\",\n      color: \"primary\",\n      children: \"\\uAC8C\\uC2DC\"\n    })]\n  });\n}","map":null,"metadata":{},"sourceType":"module"}