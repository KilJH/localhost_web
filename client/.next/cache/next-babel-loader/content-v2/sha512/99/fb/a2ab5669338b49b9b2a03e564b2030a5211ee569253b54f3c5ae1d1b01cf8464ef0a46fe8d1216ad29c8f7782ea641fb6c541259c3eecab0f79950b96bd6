{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useState } from 'react';\nimport Search from '../../search/Search';\nimport axios from 'axios';\nimport SERVER from '../../../utils/url';\nimport PlanList from './PlanList';\nexport default function PlanPage(props) {\n  const {\n    items\n  } = props;\n  const {\n    0: foundItems,\n    1: setFoundItems\n  } = useState(items);\n\n  const onSubmit = async (event, type, item) => {\n    event.preventDefault();\n    const res = await axios.post(`${SERVER}/api/plan/search`, {\n      type: type,\n      item: item\n    });\n    setFoundItems(res.data.list);\n  };\n\n  return /*#__PURE__*/_jsxs(\"div\", {\n    children: [/*#__PURE__*/_jsx(Search, {\n      options: ['title', 'nickname', 'createTime'],\n      label: ['제목', '플래너', '등록일자'],\n      onSubmit: onSubmit,\n      selectLabel: \"\\uAC80\\uC0C9\\uD560 \\uAC12\",\n      marginBottom: \"2em\"\n    }), /*#__PURE__*/_jsx(PlanList, {\n      items: foundItems.length !== 0 ? foundItems : []\n    })]\n  });\n}","map":null,"metadata":{},"sourceType":"module"}